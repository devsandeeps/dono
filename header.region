<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">

  <head>

    <meta charset="utf-8"></meta>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"></meta>
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1"></meta>
	<link href='https://fonts.googleapis.com/css?family=Noto+Sans' rel='stylesheet' type='text/css'>
	<link href='http://fonts.googleapis.com/css?family=Cinzel' rel='stylesheet' type='text/css'>

    {squarespace-headers}

    <squarespace:script src="site.js" combo="true" />
	
	

  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
  <div class="header_wrapper">
    <header id="header">
	
	<div class="header_Fixed">
		<div class="header_Fixed_content">
				<!--SITE TITLE OR LOGO-->
				<div class="Logo_Block">
					{.section website}<h1 data-content-field="site-title"><a href="/">{.section logoImageUrl}<img src="{logoImageUrl}?format=original" alt="{siteTitle}" />{.or}{siteTitle}{.end}</a></h1>{.section siteTagLine}<p style='display:none;'>{@}</p>{.end}{.end}
				</div>
				<div class="Address_Block">
					<squarespace:block-field id="Header_Address_Block" columns="1"/>
				</div>
				<div class="Search_Block">
					<squarespace:block-field id="Header_Search_Block" columns="1"/>
				</div>
				<div class="FollowUs_Block">
					<a href="/follow-us"><img src="/assets/images/follow-us-button.png" alt="Follow Us"></a><br/>
					<a href="/contact-us">Contact Us</a>
				</div>
				<div class="clear"></div>
		</div>
	</div>

        <!--MAIN NAVIGATION-->
		 <div class="div_MainNavigation_MobileIpad">
			<a href="javascript:void(1);" onclick="ShowHideMobileMenu();" class="Menu_Icon"></a>
			<a href="javascript:void(1);" onclick="ShowHideMobileMenu();">Menu</a>
		 <ul>
		 </ul>

		</div>
		<div class="div_MainNavigation_Web">
			<squarespace:navigation navigationId="mainNav" template="navigation" />
	  	</div>

    </header>
	</div>


    
	

   